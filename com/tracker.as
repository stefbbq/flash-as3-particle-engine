package com {		import flash.display.MovieClip;	import flash.events.*;			public class destination extends MovieClip {						public function destination() {			// constructor code			addEventListener(MouseEvent.MOUSE_DOWN, onMouseDown);			addEventListener(MouseEvent.MOUSE_UP, onMouseUp);		}		private function onMouseDown(e:Event){			startDrag();		}		private function onMouseUp(e:Event){			stopDrag();		}	}	}